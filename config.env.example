# ВАЖНО: Скопируйте этот файл в config.env и измените значения на актуальные
# cp config.env.example config.env

# Настройки базы данных
POSTGRES_DB=atlantis_pack_optimization
POSTGRES_USER=atlantis_user
POSTGRES_PASSWORD=atlantis_password
DATABASE_URL=postgresql://atlantis_user:atlantis_password@database:5432/atlantis_pack_optimization

# Настройки приложения
SECRET_KEY=your_secret_key_here
DEBUG=False
HOST=0.0.0.0
PORT=8000

# Настройки оптимизатора
OPTIMIZATION_ALGORITHM=hybrid
POPULATION_SIZE=100
MAX_GENERATIONS=50
MUTATION_RATE=0.1
CROSSOVER_RATE=0.8

# Настройки планирования
PLANNING_HORIZON_DAYS=30
WASTE_REDUCTION_TARGET=0.05

# Настройки интеграции
ERP_API_URL=http://localhost:8080
PARUS_API_URL=http://localhost:8081
XML_IMPORT_PATH=./data/import/
XML_EXPORT_PATH=./data/export/

# Настройки логирования
LOG_LEVEL=INFO
LOG_FILE=./logs/app.log 